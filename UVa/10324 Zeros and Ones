#include <iostream>

using namespace std;

int main(){
    string s;
    int times, first, second, Cases = 0;
    while(cin>>s){
        cout<<"Case "<<++Cases<<':'<<endl;
        int pre[s.size()];
        pre[0] = s[0]-'0';
        for(int i = 1;i<s.size();i++){
            pre[i] = pre[i-1] + s[i]-'0';
        }
        cin>>times;
        while(times--){
            cin>>first>>second;
            if(first == second) cout<<"Yes"<<endl;
            else if((pre[max(first, second)]-pre[min(first, second)])%(max(first, second)-min(first, second))==0) cout<<"Yes"<<endl;
            else cout<<"No"<<endl;
        }
    }
    return 0;
}
